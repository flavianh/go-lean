{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "clearMocks": true,
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coveragePathIgnorePatterns": ["/node_modules/"],
  "coverageThreshold": {
    "global": {
      "statements": -1
    }
  },
  "notify": true,
  "transformIgnorePatterns": ["/node_modules/"]
}
